{
  "consul": {
    "enabled": true,
    "url": "http://consul:8500",
    "service": "pusher",
    "address": "pusher",
    "port": "80",
    "pingEnabled": true,
    "pingEndpoint": "ping",
    "pingInterval": 3,
    "removeAfterInterval": 3
  },
  "fabio": {
    "enabled": true,
    "url": "http://fabio:9999",
    "service": "pusher"
  },
  "httpClient": {
    "type": "fabio",
    "retries": 3,
    "services": {}
  },
  "logger": {
    "console": {
      "enabled": true
    },
    "elk": {
      "enabled": false,
      "url": "http://elk:9200"
    },
    "file": {
      "enabled": false,
      "path": "logs/logs.txt",
      "interval": "day"
    },
    "seq": {
      "enabled": true,
      "url": "http://seq:5341",
      "apiKey": "secret"
    }
  },
  "jaeger": {
    "enabled": true,
    "serviceName": "pusher",
    "udpHost": "jaeger",
    "udpPort": 6831,
    "maxPacketSize": 65000,
    "sampler": "const",
    "excludePaths": ["/", "/ping", "/metrics"]
  },
  "prometheus": {
    "enabled": true
  },
  "rabbitMq": {
    "hostnames": [
      "rabbitmq"
    ]
  },
  "redis": {
    "connectionString": "redis",
    "instance": "pusher:"
  },
  "swagger": {
    "enabled": true,
    "reDocEnabled": false,
    "name": "pusher",
    "title": "API",
    "version": "v1",
    "routePrefix": "docs",
    "includeSecurity": true
  },
  "vault": {
    "enabled": false,
    "url": "http://vault:8200",
    "kv": {
      "enabled": false
    },
    "pki": {
      "enabled": false
    },
    "lease": {
      "mongo": {
        "enabled": false
      }
    }
  }
}